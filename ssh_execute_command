import paramiko


remote_server='192.168.200.123'
def execute_ssh(remote_server, command):
    ssh = paramiko.SSHClient()
    ssh.set_missing_host_key_policy(paramiko.AutoAddPolicy())
    ssh.connect(remote_server, port=22, username='root',paswwrod='somepasswd')
    stdin, stdout, stderr = ssh.exec_command(command)
    stdin.flush()
    return stdout.readlines()
    ssh.close()
    
    
load_details = execute_ssh(myremoteserver, 'uptime')
            server_load = load_details[0].split(':')[4]
